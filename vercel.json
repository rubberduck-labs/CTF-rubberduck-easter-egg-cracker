{
  "regions": ["arn1"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api/command_line/*.ts": {
      "memory": 128,
      "maxDuration": 2,
      "includeFiles": "api_resources/commandline/*"
    },
    "api/next_egg.ts": {
      "memory": 128,
      "maxDuration": 2,
      "includeFiles": "api_resources/reward.jpeg"
    }
  }
}